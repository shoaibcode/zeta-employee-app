{
  "name": "zeta-banking",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-scripts": "3.2.0"
  },
  "scripts": {
    "start": "npm-run-all --parallel sass transpile json-server",
    "sass": "node-sass-chokidar --watch src/styles/scss -o src/styles/css",
    "transpile": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "json-server": "json-server --watch --port=8080 src/db.json"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/runtime": "^7.7.2",
    "node-sass-chokidar": "^1.4.0",
    "npm-run-all": "^4.1.5",
    "json-server": "^0.15.1"
  }
}
